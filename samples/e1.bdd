domain d1 : int 3..5 ;domain d2 : int 0..131074 ;$dump #Z [%a:d1, %b:d2] := {%b = %a} ;dump #U [%x:d2, inf, eg, sup] := switch %x,4 : inf :: eg :: sup end ;$dump #T [%b:d2, %a:d1] := #Z [%a, %b] ;dump #t1 [%b:d2, %a:d1] := {%a = 5} ;dump #t2 [%a:d1, %b:d2] := #t1 [%b, %a]  ;domain D1 : int 3..5 ;domain D2 : int 8..12 ;domain DR : int 10..19 ;dump #S [%a:D1, %b:D2, %s:DR] := {%a + %b :: %s} ;end