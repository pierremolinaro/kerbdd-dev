$---- Les modes
#mode_repos [a:bool [5], b : bool [5]] := 
   ({a:4::0 = b:4::0} & {a:1::4 <> b:1::4})
 | ({a:3::0 = b:3::0} & {a:1::3 <> b:1::3} & {a:1::4  = b:1::4})
 | ({a:2::0 = b:2::0} & {a:1::2 <> b:1::2} & {a:2::3  = b:2::3})
 | ({a:1::0 = b:1::0} & {a:1::1 <> b:1::1} & {a:3::2  = b:3::2})
 | (                    {a:1::0 <> b:1::0} & {a:4::1  = b:4::1})
 ;

end
