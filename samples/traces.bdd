$--- Ensemble des transitions de l'automate#Trans [qs:2, a:2, qd:2] := ({qs=0} & {a=0} & {qd=1}) | ({qs=0} & {a=1} & {qd=2}) | ({qs=2} & {a=2} & {qd=1}) | ({qs=1} & {a=3} & {qd=3}) ;#initial [q:2] := {q=0} ;$--- Calcul de tous les cheminsdisplay #chemins [q:2, t0, t1:2] +=  (#initial [q] & t0) |  ?qd:2, ac:2, td0, td1:2 (     #chemins [qd, td0, td1] & #Trans [qd, ac, q] &     (td0 & {ac = t1}) ) ;end