display #S1 [f, g, h] := (if f then g else h end) = (if ~f then h else g end) ;display #S2 [f, g, h] := (if f then g else h end) = ~(if f then ~g else ~h end) ;display #T1 [g, h] := (if false then g else h end) = h ;display #S3 [f, h] := (if f then f else h end) = ~(if f then false else ~h end) ;display #T2 [f] := (if f then false else f end) = false ;display #T3 [f] := (if f then false else ~f end) = ~f ;display #S4 [f, g] := (if f then g else f end) = (if f then g else false end) ;display #S5 [f, g] := (if f then g else ~f end) = (if f then g else true end) ;display #T4 [f] := (if f then false else true end) = ~f ;display #T5 [f, g] := (if f then g else g end) = g ;display #S6 [f, h] := (if f then false else h end) = (if ~h then false else ~f end);display #S7 [f, h] := (if f then false else h end) = (if h then ~f else false end);end