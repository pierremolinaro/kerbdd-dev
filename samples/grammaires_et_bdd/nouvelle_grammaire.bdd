$--------------------------------------------------------------*$                                                              *$    Verification de la propriete LL (1) sur une grammaire     *$                                                              *$--------------------------------------------------------------*domain NTT : 0..31 ;$--- Les terminaux :$ 0 : "+"$ 1 : "-"$ 2 : "*"$ 3 : "/"$ 4 : "("$ 5 : ")"$ 6 : "id"$ 7 : "cst"#estTerminal [x:NTT] := {x <= 7} ;$--- Les non terminaux$  8 : <axiome>$  9 : <expression>$ 10 : <suiteTerme>$ 11 : <terme>$ 12 : <suiteFacteur>$ 13 : <facteur>$ 14 : <opUnaire>$ 15 : <primaire>$ 16 : <suiteFacteur1>$ 17 : <suiteFacteur2>$ 18 : <suiteTerme1>$ 19 : <suiteTerme2>$ 20 : <primaire2>$ 21 : <productionVide>#estNonTerminal [x:NTT] := {x > 7} & {x <= 21} ;#estNonTerminalOrigine [x:NTT] := {x > 7} & {x <= 15} ;$--- Productions vides#PRODUCTION_VIDE [nt:NTT] := {nt=10} | {nt=12} | {nt=14} | {nt=21} ;$--- Codage des productions de la forme <ntg> = y z :#PRODUCTION [ntg:NTT, y:NTT, z:NTT] := ({ntg= 9} & {y=11} & {z=10}) | $ <expression> = <terme> <suiteTerme>({ntg=10} & {y= 0} & {z=18}) | $ <suiteTerme> = "+" <suiteTerme1>({ntg=18} & {y=11} & {z=10}) | $ <suiteTerme1> = <terme> <suiteTerme>({ntg=10} & {y= 1} & {z=19}) | $ <suiteTerme> = "-" <suiteTerme2>({ntg=19} & {y=11} & {z=10}) | $ <suiteTerme2> = <terme> <suiteTerme>({ntg=11} & {y=13} & {z=12}) | $ <terme> = <facteur> <suiteFacteur>({ntg=12} & {y= 2} & {z=16}) | $ <suiteFacteur> = "*" <suiteFacteur1>({ntg=16} & {y=13} & {z=12}) | $ <suiteFacteur1> = <facteur> <suiteFacteur>({ntg=12} & {y= 3} & {z=17}) | $ <suiteFacteur> = "/" <suiteFacteur2>({ntg=17} & {y=13} & {z=12}) | $ <suiteFacteur2> = <facteur> <suiteFacteur>({ntg=13} & {y=14} & {z=15}) | $ <facteur> = <opUnaire> <primaire>({ntg=14} & {y= 0} & {z=21}) | $ <opUnaire> = "-" <productionVide>({ntg=14} & {y= 1} & {z=21}) | $ <opUnaire> = "+" <productionVide>({ntg=15} & {y= 6} & {z=21}) | $ <primaire> = "id" <productionVide>({ntg=15} & {y= 7} & {z=21}) | $ <primaire> = "cst" <productionVide>({ntg=15} & {y= 4} & {z=20}) | $ <primaire> = "(" <primaire2>({ntg=20} & {y= 9} & {z= 5}) | $ <primaire2> = <expression> ")"({ntg= 8} & {y= 9} & {z=21}) ; $ <axiome> = <expression> <productionVide>$--- Axiome de la grammaire#AXIOME [nt:NTT] := {nt=8} ;$--- verificationsinclude "inclusion_verifSurGrammaire.bdd" ;$--- Premiersinclude "inclusion_calcul_PREMIERS.bdd" ;$--- Suivantsinclude "inclusion_calcul_SUIVANTS.bdd" ;$--- Analyser la grammaireinclude "inclusion_test_LL1.bdd" ;include "inclusion_test_precedence_operateurs.bdd" ;end