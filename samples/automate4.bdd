$--- Ensemble des transitions du graphe#Trans [q3s, q2s, q1s, q0s, q3d, q2d, q1d, q0d] :=   (~q3s & ~q2s & ~q1s & ~q0s & ~q3d & ~q2d & ~q1d &  q0d) |   (~q3s & ~q2s & ~q1s &  q0s & ~q3d & ~q2d &  q1d & ~q0d) |   (~q3s & ~q2s &  q1s & ~q0s & ~q3d & ~q2d &  q1d &  q0d) |   (~q3s & ~q2s &  q1s &  q0s & ~q3d &  q2d & ~q1d & ~q0d) |   (~q3s &  q2s & ~q1s & ~q0s & ~q3d &  q2d & ~q1d &  q0d) |   (~q3s &  q2s & ~q1s & ~q0s & ~q3d & ~q2d &  q1d & ~q0d) |   (~q3s &  q2s & ~q1s &  q0s & ~q3d &  q2d &  q1d & ~q0d) |   (~q3s &  q2s &  q1s &  q0s & ~q3d &  q2d & ~q1d &  q0d) |   (~q3s &  q2s &  q1s & ~q0s &  q3d & ~q2d & ~q1d & ~q0d) |   ( q3s & ~q2s & ~q1s & ~q0s &  q3d & ~q2d & ~q1d &  q0d) |   ( q3s & ~q2s & ~q1s &  q0s &  q3d & ~q2d &  q1d & ~q0d) |   ( q3s & ~q2s & ~q1s &  q0s &  q3d & ~q2d &  q1d &  q0d) |   ( q3s & ~q2s &  q1s &  q0s & ~q3d &  q2d & ~q1d & ~q0d) |   ( q3s & ~q2s &  q1s &  q0s &  q3d &  q2d & ~q1d & ~q0d) |   ( q3s &  q2s & ~q1s & ~q0s &  q3d &  q2d & ~q1d &  q0d) |   ( q3s &  q2s & ~q1s &  q0s &  q3d &  q2d &  q1d & ~q0d) |   ( q3s &  q2s &  q1s & ~q0s &  q3d &  q2d &  q1d &  q0d) |   ( q3s &  q2s & ~q1s &  q0s & ~q3d &  q2d & ~q1d & ~q0d) ;   $--- Etat initial#Initial [q3s, q2s, q1s, q0s] := ~q3s & ~q2s & ~q1s & ~q0s ;display ;$--- Calcul des Žtats accessibles par plus petit point fixe#Acces [q3, q2, q1, q0] += #Initial [q3, q2, q1, q0] |     ? q3s, q2s, q1s, q0s (#Trans [q3s, q2s, q1s, q0s, q3, q2, q1, q0]                                & #Acces [q3s, q2s, q1s, q0s]) ;$--- Calcul par itŽration quadratique#Quad [q3s, q2s, q1s, q0s, q3d, q2d, q1d, q0d] ::=      #Trans [q3s, q2s, q1s, q0s, q3d, q2d, q1d, q0d] |      ((q3s = q3d) & (q2s = q2d) & (q1s = q1d) & (q0s = q0d))  ::        ? t3, t2, t1, t0       (#Quad [q3s, q2s, q1s, q0s, t3, t2, t1, t0] &          #Quad [t3, t2, t1, t0, q3d, q2d, q1d, q0d]) ;#Acc [q3, q2, q1, q0] := ?t3, t2, t1, t0 (#Initial [t3, t2, t1, t0]                        & #Quad [t3, t2, t1, t0, q3, q2, q1, q0]) ;$--- Ensemble des Žtats morts#NoSucc [q3, q2, q1, q0] := #Acces [q3, q2, q1, q0]                   & ~ ? t3, t2, t1, t0 #Trans [q3, q2, q1, q0, t3, t2, t1, t0] ; $--- Ensemble des Žtats ne pouvant conduire que vers un Žtat mort#DeadLock [q3, q2, q1, q0] += #Acces [q3, q2, q1, q0]    & !t3, t2, t1, t0 (       (#Acces [q3, q2, q1, q0] & #Trans [q3, q2, q1, q0, t3, t2, t1, t0])          -> #DeadLock [t3, t2, t1, t0]       ) ;end