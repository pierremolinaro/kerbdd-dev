$--- Ensemble des transitions du premier automate#Trans1 [qs:2, t, qd:2] := ({qs=0} & {t=0} & {qd=1}) | ({qs=1} & {t=1} & {qd>=2}) ;#accessible1 [qs:2] := true ;#initial1 [qs:2] := {qs=0} ;#terminal1 [qs:2] := {qs=3} ;$--- Ensemble des transitions du second automate#Trans2 [qs:3, t, qd:3] :=  ({qs=0} & {t=0} & {qd=1}) | ({qs=0} & {t=0} & {qd=2}) | ({qs=1} & {t=1} & {qd=3}) | ({qs=2} & {t=1} & {qd=4}) ;#accessible2 [qs:3] := {qs <= 4} ;#initial2 [qs:3] := {qs=0} ;#terminal2 [qs:3] := {qs=4} ;$--- Calcul de la plus grande bisimulation$---- Equivalence fortedisplay #bisimul [q1:2, q2:3] -=  #accessible1 [q1] & #accessible2 [q2] &  (#initial1 [q1] = #initial2 [q2]) &  (#terminal1 [q1] = #terminal2 [q2]) &  ! q1d:2, a (#Trans1 [q1, a, q1d] -> ?q2d:3 (#Trans2 [q2, a, q2d] & #bisimul [q1d, q2d])) &  ! q2d:3, a (#Trans2 [q2, a, q2d] -> ?q1d:2 (#Trans1 [q1, a, q1d] & #bisimul [q1d, q2d])) ;$---- Equivalence forte#eqForte := !q1:2 (#accessible1 [q1] -> ?q2:3 (#accessible2 [q2] & #bisimul [q1, q2])) &       !q2:3 (#accessible2 [q2] -> ?q1:2 (#accessible1 [q1] & #bisimul [q1, q2])) ;end