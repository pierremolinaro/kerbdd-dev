$--- Ensemble des transitions du graphe#Trans [qs:3, qd:3] :=   ({qs=0} & {qd=1}) | ({qs=1} & {qd=2}) | ({qs=2} & {qd=3}) |   ({qs=3} & {qd=4}) | ({qs=2} & {qd=1}) | ({qs=5} & {qd=6}) |   ({qs=6} & {qd=5}) ;$--- Etat initial#Initial [q:3] := {q=0} ;$--- Calcul des Žtats accessibles par plus petit point fixedisplay #Acces [q:3] += #Initial [q] | ? t:3 (#Acces [t] & #Trans [t, q]) ;$--- Mme formule, calcul du plus grand point fixedisplay #Acces2 [q:3] -= #Initial [q] | ? t:3 (#Acces2 [t] & #Trans [t, q]) ;$--- Calcul des Žtats non-accessibles par plus grand point fixedisplay #NonAccessibles [q:3] -= ~(#Initial [q] | ? t:3 (~#NonAccessibles [t] & #Trans [t, q])) ;display #NonAccessibles2 [q:3] -= ~#Initial [q] & !t:3 (#Trans [t, q] -> #NonAccessibles2 [t]) ;$--- Ensemble des Žtats mortsdisplay #NoSucc [q:3] := #Acces [q] & ~ ? t:3 #Trans [q, t] ; $--- Ensemble des Žtats ne pouvant conduire que vers un Žtat mortdisplay #DeadLock [q:3] += #Acces [q] &             !t:3 ((#Acces [q] & #Trans [q, t]) -> #DeadLock [t]) ;display #DeadLock2 [q:3] += #Acces [q] & ~?t:3 (#Trans [q, t] & ~#DeadLock2 [t]) ;end